<!-- News Letter Start -->
<section>
    <div class="newsletter">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12">
                    <div class="newsletter-inner center-sm">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="newsletter-title">
                                    <h2 class="main_title">Sign up to receive 10% off* your first order</h2>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <form>
                                    <div class="newsletter-box">
                                        <input type="email" placeholder="Email Here...">
                                        <button title="Subscribe" class="btn-color">Subscribe</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- News Letter End -->


    <div class="wrapper homepage2">
        <header>
            <div class="top-bar">
                <div class="container">
                    <div class="col-xs-12 col-sm-6">
                        <ul class="info-inline">
                            <li>
                                <i class="fa fa-phone"></i> {{ phone }}
                            </li>
                            <li>
                                <i class="fa fa-envelope"></i> {{ email }}
                            </li>
                        </ul>
                    </div>
                    <div class="col-xs-12 col-sm-6">
                        <ul class="social-icons small">
                            {% if config_social_facebook != '' %}
                            <li>
                                <a href="{{ config_social_facebook }}" class="fa fa-facebook-square"></a>
                            </li>
                            {% endif %}
                            {% if config_social_twitter != '' %}
                            <li>
                                <a href="{{ config_social_twitter }}" class="fa fa-twitter-square"></a>
                            </li>
                            {% endif %}
                            {% if config_social_google != '' %}
                            <li>
                                <a href="{{ config_social_google }}" class="fa fa-google-plus-square"></a>
                            </li>
                            {% endif %}
                            {% if config_social_instagram != '' %}
                            <li>
                                <a href="{{ config_social_instagram }}" class="fa fa-instagram"></a>
                            </li>
                            {% endif %}
                            {% if config_social_line != '' %}
                            <li>
                                <a href="{{ config_social_line }}" class="">
                                    <img src="image/line.png" alt="" style="width:15px;vertical-align: sub;">
                                </a>
                            </li>
                            {% endif %}
                            {% if config_social_twitter != '' %}
                            <li>
                                <a href="{{ config_social_twitter }}" class="fa fa-twitter"></a>
                            </li>
                            {% endif %}
                            {% if config_social_youtube != '' %}
                            <li>
                                <a href="{{ config_social_youtube }}" class="fa fa-youtube-square"></a>
                            </li>
                            {% endif %}
                        </ul>
                    </div>
                </div>
            </div>
            <div class="container header-row">
                <div class="col-xs-12 col-md-3">
                    <div class="buttons-holder">
                        {{ language }}
                        {# <div class="btn-group le-dropdown">
                            <a class="dropdown-toggle" href="#" data-toggle="dropdown">
                      engish
                    </a>
                            <ul class="dropdown-menu">
                                <li><a href="#">turkish</a></li>
                                <li><a href="#">arabic</a></li>
                                <li><a href="#">french</a></li>
                            </ul>
                        </div> #}
                        {% if currency %}
                        <div class="btn-group le-dropdown">
                            {# <a class="dropdown-toggle" href="#" data-toggle="dropdown">
                                <i class="fa fa-usd"></i> usd
                            </a>
                            <ul class="dropdown-menu">
                                <li><a href="#"><i class="fa fa-try"></i> try</a></li>
                                <li><a href="#"><i class="fa fa-gbp"></i> gbp</a></li>
                                <li><a href="#"><i class="fa fa-euro"></i> eur</a></li>
                            </ul> #}
                            {{ currency }}
                        </div>
                        {% endif %}

                    </div>
                </div>
                <div class="col-xs-12 col-md-6">
                    <div class="logo">
                        <a href="index.php?route=common/home">
                            <img src="../image/catalog/Logo/kappanewlogo.png" alt="logo" style="height: 70px;" />
                            <!--  <span>PAN</span> -->
                        </a>
                    </div>
                </div>
                <div class="col-xs-12 col-md-3">
                    <ul class="link-list inline">
                        <li><a href="{{account}}">my account</a></li>
                        <li><a href="{{wishlist}}">my wishlist</a></li>
                        {% if logged %}
                        <li><a href="{{logout}}">log out</a></li>
                        {% else %}
                        <li><a href="{{login}}">log in</a></li>
                        {% endif %}
                    </ul>
                </div>
            </div>
            <div class="container ">
                <div class="top-nav-holder">
                    <div class="row">
                        <div class="col-xs-12 col-md-6 col-lg-8  nav-menu top-menu-holder">

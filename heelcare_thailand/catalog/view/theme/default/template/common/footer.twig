<!-- FOOTER START -->
<div class="footer">
    <div class="container">
        <div class="footer-inner">
            <div class="footer-middle">
                <div class="row mb-60">
                    <div class="col-md-4 f-col footer-middle-left">
                        <div class="f-logo">
                            <a href="index.php" class="">
                                <img src="catalog/view/theme/default/assets/images/logo.png" alt="Eshoper">
                            </a>
                        </div>
                        <div class="footer-static-block"> <span class="opener plus"></span>
                            <ul class="footer-block-contant address-footer">
                                <li class="item"> <i class="fa fa-map-marker"> </i>
                                    <p>{{ address }}</p>
                                </li>
                                <li class="item"> <i class="fa fa-envelope"> </i>
                                    <p>{{ email }}</p>
                                </li>
                                <li class="item"> <i class="fa fa-phone"> </i>
                                    <p>{{ phone }}</p>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-8 footer-middle-right">
                        <div class="row mb-40">
                            <div class="icon-newsletter">
                                <div class="col-lg-5 col-md-12">
                                    <div class="footer_social pt-xs-15 center-sm">
                                        <ul class="social-icon">
                                            <li><a title="Facebook" class="facebook"><i class="fa fa-facebook"> </i></a></li>
                                            <li><a title="Twitter" class="twitter"><i class="fa fa-twitter"> </i></a></li>
                                            <li><a title="googleplus" class="googleplus"><i class="fa fa-google-plus"></i></a></li>
                                            <li><a title="RSS" class="rss"><i class="fa fa-rss"> </i></a></li>
                                            <li><a title="Pinterest" class="pinterest"><i class="fa fa-pinterest"> </i></a></li>
                                            <li><a title="Linkedin" class="linkedin"><i class="fa fa-linkedin"> </i></a></li>
                                            <li><a title="youtube" class="youtube"><i class="fa fa-youtube"></i></a></li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="col-lg-7 col-md-12">
                                    <div class="newsletter-inner center-sm">
                                        <div class="row">
                                            <div class="col-md-1">
                                                <div class="newsletter-title">
                                                </div>
                                            </div>
                                            <div class="col-md-11">
                                                <form>
                                                    <div class="newsletter-box">
                                                        <input type="email" placeholder="Email Here...">
                                                        <button title="Subscribe" class="btn-color">Subscribe</button>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                          {% for cat in category %}
                            <div class="col-md-3 f-col">
                                <div class="footer-static-block"> <span class="opener plus"></span>
                                    <a href="{{cat.href}}"><h5 class="title">{{cat.name}}</h5></a>
                                    {% if cat.product | length > 0 %}
                                    <ul class="footer-block-contant link">
                                      {% for pro in cat.product %}
                                        <li><a href="{{pro.href}}">{{pro.name}}</a></li>  
                                      {% endfor %}
                                    </ul>
                                    {% endif %}
                                </div>
                            </div>
                          {% endfor %}
                        </div>
                    </div>
                </div>
            </div>
            <hr>
            <div class="footer-bottom align-center">

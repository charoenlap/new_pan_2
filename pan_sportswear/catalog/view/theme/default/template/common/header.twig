    <header>
        <div class="top-bar">
            <div class="container">
                <div class="col-xs-12 col-sm-6">
                    <ul class="info-inline">
                        <li>
                            <i class="fa fa-phone"></i> {{ phone }}, 095-762-5283
                        </li>
                        <li>
                            <i class="fa fa-envelope"></i> {{ email }}
                        </li>
                    </ul>
                </div>
                <div class="col-xs-12 col-sm-6">
                    <ul class="social-icons small">
                        {% if config_social_facebook != '' %}
                        <li>
                            <a href="{{ config_social_facebook }}" class="fa fa-facebook-square"></a>
                        </li>
                        {% endif %}
                        {% if config_social_twitter != '' %}
                        <li>
                            <a href="{{ config_social_twitter }}" class="fa fa-twitter-square"></a>
                        </li>
                        {% endif %}
                        {% if config_social_google != '' %}
                        <li>
                            <a href="{{ config_social_google }}" class="fa fa-google-plus-square"></a>
                        </li>
                        {% endif %}
                        {% if config_social_instagram != '' %}
                        <li>
                            <a href="{{ config_social_instagram }}" class="fa fa-instagram"></a>
                        </li>
                        {% endif %}
                        {% if config_social_line != '' %}
                        <li>
                            <a href="{{ config_social_line }}" class="">
                                <img src="image/line.png" alt="" style="width:15px;vertical-align: sub;">
                            </a>
                        </li>
                        {% endif %}
                        {% if config_social_twitter != '' %}
                        <li>
                            <a href="{{ config_social_twitter }}" class="fa fa-twitter"></a>
                        </li>
                        {% endif %}
                        {% if config_social_youtube != '' %}
                        <li>
                            <a href="{{ config_social_youtube }}" class="fa fa-youtube-square"></a>
                        </li>
                        {% endif %}
                    </ul>
                </div>
            </div>
        </div>
        <div class="container header-row">
            <div class="row">
                <div class="col-md-2">
                    <a href="#">
                        <img src="catalog/view/theme/default/assets/images/banner4product_1.jpg" alt="logo" style="width:100%;" />
                    </a>
                </div>
                <div class="col-md-10">
                    <div class="row">
                        <div class="col-md-5 col-md-offset-7">
                            <ul class="link-list inline">
                                <li>{{ language }}</li>
                                <li><a href="{{ account }}">my account</a></li>
                                <li><a href="{{ wishlist }}">my wishlist</a></li>
                                <li><a href="{{ login }}">log in</a></li>    
                            </ul>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="top-nav-holder">
                                <div class="row">
                                    <div class="col-xs-12 col-md-6 col-lg-9  nav-menu top-menu-holder">
                                        {{menu}}
                                    </div>
                                    <div class="col-xs-12 col-md-3 col-lg-1 no-margin search-holder">
                                        <a href="index.php?route=product/search" class="search-button" style="height:49px;width:49px;text-align:center;display:table;float:right;background:#e5e5e5;">
                                            <i class="fa fa-search" style="display:table-cell;vertical-align:middle;font-size:16px;"></i>
                                        </a>

<div class="panel panel-default">
  <div class="panel-heading">
    <div class="pull-right"><a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-calendar"></i> <i class="caret"></i></a>
      <ul id="range" class="dropdown-menu dropdown-menu-right">
        <li><a href="day">{{ text_day }}</a></li>
        <li class="active"><a href="week">{{ text_week }}</a></li>
        <li><a href="month">{{ text_month }}</a></li>
        <li><a href="year">{{ text_year }}</a></li>
      </ul>
    </div>
    <h3 class="panel-title"><i class="fa fa-bar-chart-o"></i> {{ heading_title }}</h3>
  </div>
  <div class="panel-body">
  	<input type="hidden" id="datachart" value="{{ result }}">
    <div id="chartage-sale" style="width: 100%; height: 260px;"></div>
  </div>
</div>
{# <script type="text/javascript" src="view/javascript/jquery/flot/jquery.flot.js"></script>  #}
{# <script type="text/javascript" src="view/javascript/jquery/flot/jquery.flot.resize.min.js"></script> #}
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
  google.charts.load('current', {'packages':['corechart']});
  google.charts.setOnLoadCallback(drawChart);

  function drawChart() {
  	var datachart = decodeURIComponent(document.getElementById('datachart').value);
  	var jsondata = jQuery.parseJSON(datachart);
  	console.log(jsondata);
    var data = google.visualization.arrayToDataTable(jsondata);

    var options = {
      curveType: 'function',
      legend: { position: 'bottom' },
    };

    var chart = new google.visualization.LineChart(document.getElementById('chartage-sale'));

    chart.draw(data, options);
  }
</script>